{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "make",
			"command": "make",
			"args": ["-e", "CFLAGS=' -g -O0 '"],
			"options": {
				"cwd": "${workspaceFolder}/src/output"
			},
			"group": "build"
		},
		{
			"type": "shell",
			"label": "make clean",
			"command": "make",
			"args": ["clean"],
			"options": {
				"cwd": "${workspaceFolder}/src/output"
			},
			"group": "build"
		},
		{
			"type": "shell",
			"label": "make install",
			"command": "make",
			"args": ["install"],
			"options": {
				"cwd": "${workspaceFolder}/src/output"
			},
			"group": "build"
		}
	]
}